{
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "run:shared": "cd shell && npm run serve",
    "run:account": "cd app1 && npm run serve",
    "run:web": "cd app2 && npm run serve",
    "install:shared": "cd shell && npm install",
    "install:account": "cd app1 && npm install",
    "install:web": "cd app2 && npm install",
    "all": "npm-run-all --parallel run:*",
    "install": "npm-run-all --serial install:*"
  }
}
